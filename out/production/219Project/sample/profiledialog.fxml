<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.shape.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>

<BorderPane id="loginDialogPane" fx:id="loginBorderPane" maxHeight="500.0" maxWidth="750.0" minHeight="500.0" minWidth="750.0" prefHeight="500.0" prefWidth="750.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.ProfileDialogController">
   <center>
      <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" translateX="-40.0" BorderPane.alignment="CENTER">
         <children>
            <TextField id="usernameTextField" fx:id="profileUsernameField" maxHeight="25.0" maxWidth="200.0" minHeight="25.0" minWidth="200.0" prefHeight="25.0" prefWidth="200.0" promptText="Username" translateX="50.0">
               <VBox.margin>
                  <Insets top="50.0" />
               </VBox.margin>
            </TextField>
            <PasswordField id="passwordTextField" fx:id="profilePasswordField" maxHeight="25.0" maxWidth="200.0" minHeight="25.0" minWidth="200.0" prefHeight="25.0" prefWidth="200.0" promptText="Password" translateX="50.0">
               <VBox.margin>
                  <Insets top="50.0" />
               </VBox.margin>
            </PasswordField>
         </children>
      </VBox>
   </center>
   <bottom>
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" translateX="-75.0" translateY="-75.0" BorderPane.alignment="CENTER">
         <children>
            <Button id="loginDialogLogin" fx:id="createButton" alignment="CENTER" mnemonicParsing="false" onAction="#createProfile" text="Create" translateX="75.0">
               <HBox.margin>
                  <Insets top="10.0" />
               </HBox.margin>
            </Button>
            <Button id="loginDialogCancel" fx:id="profileDialogCancel" cancelButton="true" mnemonicParsing="false" onAction="#dialogCancel" text="Cancel" translateX="100.0">
               <HBox.margin>
                  <Insets top="10.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
   </bottom>
</BorderPane>
